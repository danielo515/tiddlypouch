{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [ "plugins/markdown" ],
    "templates": {
        "outputSourceFiles": true,
        "systemName"            : "TiddlyPouch",
        "footer"                : "",
        "copyright"             : "Copyright © 2014 Danielo Rodríguez Rivero",
        "navType"               : "vertical",
        "theme"                 : "sandstone",
        "linenums"              : true,
        "collapseSymbols"       : false,
        "inverseNav"            : true,
        "highlightTutorialCode" : true
    },
    "opts": {
      "template": "./node_modules/ink-docstrap/template",
      "encoding": "utf8",
      "private": true
    }
}
