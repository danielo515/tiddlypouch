{
    "name": "tiddlypouch",
    "version": "0.27.1-19",
    "author": "Danielo Rodr√≠guez Rivero",
    "description": "Tiddlypouch brings all the power of Pouchdb into tiddlywiki. It is a key part of the NoteSelf project",
    "license": "BSD-2-Clause",
    "private": true,
    "scripts": {
        "changelog": "standard-changelog",
        "commitmsg": "validate-commit-msg",
        "deploy": "gulp --production && git push --tags",
        "build": "gulp",
        "watch": "gulp watch",
        "bump": "gulp bump_version --minor",
        "start": "npm run build && npm run TW",
        "TW": "nodemon ./node_modules/tiddlywiki/tiddlywiki.js ./tiddlypouchwiki --verbose --server 8087 $:/core/save/all text/plain text/html",
        "dev-docs": "tiddlywiki ./documentationwiki --verbose --server 8088 $:/core/save/all text/plain text/html"
    },
    "contributors": [
        {
            "name": "Individual contributors"
        },
        {
            "name": "Entity contributors"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/danielo515/tiddlypouch"
    },
    "bugs": {
        "url": "https://github.com/danielo515/tiddlypouch/issues"
    },
    "keywords": [
        "tiddlywiki",
        "tw5",
        "wiki",
        "knowledge management",
        "browser"
    ],
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
        "@babel/preset-env": "^7.9.5",
        "@babel/register": "^7.9.0",
        "conventional-changelog": "^1.1.3",
        "cross-env": "^7.0.2",
        "del": "^2.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-debug": "^4.0.0",
        "gulp-esprima": "^0.1.0",
        "gulp-if": "^3.0.0",
        "gulp-jsdoc3": "^3.0.0",
        "gulp-replace": "^1.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.0",
        "gulp-tag-version": "^1.3.0",
        "gulp-uglify": "^3.0.2",
        "gulp-watch": "^5.0.1",
        "gulp4-run-sequence": "^1.0.1",
        "husky": "^0.14.0",
        "ink-docstrap": "^1.3.2",
        "is-there": "^4.4.2",
        "nodemon": "^2.0.3",
        "semver": "^5.1.0",
        "standard-changelog": "^1.0.1",
        "validate-commit-msg": "^2.12.2",
        "yargs": "^3.0.0"
    },
    "dependencies": {
        "tiddlywiki": "^5.1.21",
        "tw-pouchdb": "*"
    }
}
